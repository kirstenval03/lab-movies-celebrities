{{!-- views/celebrities/celebrities.hbs --}}

<h2>LIST OF CELEBRITIES</h2>
<a href="/">Back to home-page</a>
{{!-- since we passed allTheBooksFromDB as `books`,
we can iterate over it here with #each--}}

{{#each celebrities}}
  <div>
    <h3>{{name}}</h3>
     <form action="/celebrities/delete/{{_id}}" method="GET">
      <button>Delete</button>
    </form>
 

  </div>
{{/each}}